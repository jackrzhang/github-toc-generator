{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "generators": true,
    "restParams": true,
    "spread": true
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0
  }
}